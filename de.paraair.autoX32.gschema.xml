            </description>
        </key>  
        <key name='window-height' type='i'>
            <default>100</default>
            <summary>Window height</summary>
            <description>
                Window height
            </description>
        </key>  
        <key name='window-left' type='i'>
            <default>100</default>
            <summary>Window left</summary>
            <description>
                Window left
            </description>
        </key>  
        <key name='window-top' type='i'>
            <default>100</default>
            <summary>Window top</summary>
            <description>
                Window top
            </description>
        </key>  
        <key name='recent-projects' type='as'>
            <default>[]</default>
            <summary>Recently used project locations</summary>
            <description>
                A list of recently used project locations
            </description>
        </key>
    </schema>

</schemalist>

